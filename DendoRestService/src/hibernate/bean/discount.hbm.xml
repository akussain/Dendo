<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Models.Discount" table="discounts">
        <id column="id" name="id" type="java.lang.Integer" />
        <property column="title" name="title" type="java.lang.String" />
        <property column="description" name="desc" type="java.lang.String" />
        <property column="old_price" name="oldPrice" type="java.lang.Double" />
        <property column="new_price" name="newPrice" type="java.lang.Double" />
        <property column="source_portal" name="sourcePortal" type="java.lang.String" />
        <property column="isActive" name="isActive" type="java.lang.Integer" />
        <property column="image_url" name="imageUrl" type="java.lang.String" />
        <property column="date_created" name="createdDate" type="java.util.Date" />
        <property column="date_started" name="startedDate" type="java.util.Date" />
        <property column="date_ended" name="endedDate" type="java.util.Date" />
    </class>
</hibernate-mapping>